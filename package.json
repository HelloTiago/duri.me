{
  "name": "duri.me",
  "version": "0.1.0",
  "scripts": {
    "start": "webpack-dev-server --hot --inline --progress",
    "prebuild": "npm run clean",
    "build": "NODE_ENV=production webpack -p",
    "postbuild": "npm run copy",
    "clean": "rimraf build",
    "copy": "cp -r public/* build && surge ./build --domain duri.me"
  },
  "author": "Tiago Duarte <hello@tiagoduarte.com> (http://tiagoduarte.com/)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hellotiago/duri.me"
  },
  "license": "MIT",
  "dependencies": {
    "clipboard-js": "^0.2.0",
    "copy-to-clipboard": "^3.0.5",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-dropzone": "^3.5.1",
    "react-router": "^2.4.1"
  },
  "devDependencies": {
    "babel-core": "^6.9.1",
    "babel-eslint": "^6.0.2",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "css-loader": "^0.23.1",
    "eslint": "^2.12.0",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-react": "^5.0.1",
    "file-loader": "^0.8.5",
    "node-sass": "^3.4.2",
    "rimraf": "^2.5.2",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-notifier": "^1.3.0"
  }
}
