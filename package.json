{
  "name": "react-boilerplate",
  "version": "0.3.0",
  "scripts": {
    "start": "webpack-dev-server --hot --inline --progress",
    "prebuild": "npm run clean",
    "build": "NODE_ENV=production webpack -p",
    "postbuild": "npm run copy",
    "clean": "rimraf build",
    "copy": "cp -r public/* build && surge ./build --domain v2.duri.me"
  },
  "author": "Riccardo Bartoli <info@rblab.com> (http://rblab.com/)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rbartoli/react-boilerplate"
  },
  "license": "MIT",
  "dependencies": {
    "clipboard-js": "^0.2.0",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-dropzone": "^3.4.0",
    "react-router": "^2.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.7.6",
    "babel-eslint": "^6.0.2",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "css-loader": "^0.23.1",
    "eslint": "^2.7.0",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-react": "^5.0.1",
    "file-loader": "^0.8.5",
    "node-sass": "^3.4.2",
    "rimraf": "^2.5.2",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1",
    "webpack-notifier": "^1.3.0"
  }
}
